{
  "manifestVersion": 1,
  "id": "tools",
  "version": "0.1.2",
  "name": "Anchore Task",
  "publisher": "farrellsoft",
  "description": "Integrate with Anchore Container scanning",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "images/anchore128.png"
  },
  "scopes": ["vso.work", "vso.code_write", "vso.build_execute"],
  "categories": ["Azure Pipelines"],
  "content": {
    "details": {
      "path": "readme.md"
    },
    "license": {
      "path": "eula.md"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/xximjasonxx/anchore-task-extension"
  },
  "files": [
    {
      "path": "dist/"
    }
  ],
  "contributionTypes": [
    {
      "id": "build-task",
      "name":"scan image"
    }
  ]
}
